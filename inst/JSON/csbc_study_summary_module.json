{
  "header_text": "Please hover your cursor over the plots to view more information. You can also zoom into parts of the plot.",
  "study_table": {
    "join_column": "grantName",
    "tables": {
      "studies": {
        "columns": [
          "grantName",
          "consortium"
        ]
      },
      "files": {
        "columns": [
          "grantName",
          "assay",
          "id"
        ]
      },
      "tools": {
        "columns": [
          "grantName",
          "toolName"
        ]
      },
      "merged": {
        "columns": [
          {
            "display_name": "Grant Name",
            "name": "grantName"
          },
          {
            "display_name": "Consortium",
            "name": "consortium"
          },
          {
            "display_name": "Assays",
            "name": "assay"
          },
          {
            "display_name": "Files",
            "name": "id"
          },
          {
            "display_name": "Tools",
            "name": "toolName"
          }
        ]
      }
    }
  },
  "study_name_column": "Grant Name",
  "study_summary": {
    "table": "studies",
    "filter_column": "grantName",
    "columns": [
      {
        "name": "grantId",
        "type": "character",
        "display_name": "Unique Study ID"
      }
    ]
  },
  "study_timeline_plot": {
    "table": "files",
    "filter_column": "grantName",
    "columns": [
      {
        "name": "grantName",
        "display_name": "Grant Name",
        "type": "character"
      },
      {
        "name": "resourceType",
        "display_name": "Resource Type",
        "type": "character",
        "na_replace": "Not Annotated"
      },
      {
        "name": "year",
        "display_name": "Year",
        "type": "integer"
      },
      {
        "name": "month",
        "display_name": "Month",
        "type": "factor"
      }
    ],
    "plot": {
      "x": "Grant Name",
      "fill": "Resource Type",
      "facet": [
        "Year",
        "Month"
      ]
    },
    "tooltips": [
      "count",
      "fill"
    ],
    "empty_table_message": "The investigator/investigators has/have not uploaded any files yet. Please check back later."
  },
  "data_focus_plot": {
    "table": "files",
    "filter_column": "grantName",
    "columns": [
      {
        "name": "grantName",
        "display_name": "Study Name",
        "type": "character"
      },
      {
        "name": "assay",
        "display_name": "Assays Used",
        "type": "character",
        "na_replace": "Not Annotated"
      },
      {
        "name": "resourceType",
        "display_name": "Resource Added",
        "type": "character",
        "na_replace": "Not Annotated"
      },
      {
        "name": "gender",
        "display_name": "Gender",
        "type": "character",
        "na_replace": "Not Annotated"
      },
      {
        "name": "dataset",
        "display_name": "Dataset",
        "type": "character",
        "na_replace": "Not Annotated"
      }
    ],
    "plot": {
      "x": "Study Name",
      "fill": [
        "Assays Used",
        "Resource Added",
        "Gender",
        "Dataset"
      ]
    },
    "tooltips": [
      "count",
      "fill"
    ],
    "empty_table_message": "The investigators have not uploaded data for this study yet. Please check back later."
  },
  "annotation_activity_plot": {
    "table": "files",
    "filter_column": "grantName",
    "columns": [
      {
        "name": "consortium",
        "display_name": "Consortium",
        "type": "character",
        "na_replace": "pendingAnnotation"
      },
      {
        "name": "assay",
        "display_name": "Assay",
        "type": "character",
        "na_replace": "pendingAnnotation"
      },
      {
        "name": "year",
        "display_name": "Year",
        "type": "integer"
      }
    ],
    "plot": {
      "x": "Consortium",
      "y": "Count",
      "fill": "Assay",
      "facet": "Year"
    },
    "empty_table_message": "The investigators have not uploaded any files yet. Please check back later.",
    "tooltips": [
      "Count",
      "fill"
    ]
  },
  "publication_status_plot": {
    "table": "publications",
    "filter_column": "grantName",
    "columns": [
      {
        "name": "publicationYear",
        "display_name": "Year",
        "type": "integer"
      },
      {
        "name": "grantName",
        "display_name": "Grant Name",
        "type": "character"
      }
    ],
    "plot": {
      "x": "Year",
      "fill": "Grant Name"
    },
    "empty_table_message": "This study has no associated publications yet.",
    "tooltips": [
      "count",
      "fill"
    ]
  }
}
