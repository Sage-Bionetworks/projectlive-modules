{
  "merged_table": {
    "filter_column": "studyName",
    "join_column": "studyName",
    "tables": [
      "studies",
      "files",
      "tools"
    ],
    "columns": [
      [
        "studyName",
        "studyStatus",
        "dataStatus",
        "studyLeads",
        "diseaseFocus",
        "summary",
        "consortium",
        "studyId"
      ],
      [
        "studyName",
        "individualID",
        "specimenID",
        "assay",
        "id",
        "resourceType",
        "year",
        "month",
        "tumorType",
        "species",
        "projectId"
      ],
      [
        "studyName",
        "softwareName"
      ]
    ]
  },
  "study_table": {
    "group_columns": [
      "studyName",
      "studyLeads",
      "studyStatus",
      "dataStatus",
      "diseaseFocus"
    ],
    "count_columns": [
      "individualID",
      "specimenID",
      "assay",
      "id",
      "softwareName"
    ],
    "columns": [
      {
        "display_name": "Name",
        "name": "studyName"
      },
      {
        "display_name": "Leads",
        "name": "studyLeads",
        "type": "list:character"
      },
      {
        "display_name": "Study Status",
        "name": "studyStatus"
      },
      {
        "display_name": "Data Status",
        "name": "dataStatus"
      },
      {
        "display_name": "Disease Focus",
        "name": "diseaseFocus",
        "type": "list:character"
      },
      {
        "display_name": "Individuals",
        "name": "individualID"
      },
      {
        "display_name": "Specimens",
        "name": "specimenID"
      },
      {
        "display_name": "Assays",
        "name": "assay"
      },
      {
        "display_name": "Files",
        "name": "id"
      },
      {
        "display_name": "Tools",
        "name": "softwareName"
      }
    ],
    "id_column": "Name"
  },
  "annotation_activity": {
    "table": "merged_table",
    "empty_table_message": "The investigators have not uploaded any files yet. Please check back later.",
    "tooltips": [
      "Count",
      "fill"
    ],
    "columns": [
      {
        "name": "studyLeads",
        "display_name": "Study Leads",
        "type": "list:character",
        "na_replace": "pendingAnnotation"
      },
      {
        "name": "assay",
        "display_name": "Assay",
        "type": "character",
        "na_replace": "pendingAnnotation"
      },
      {
        "name": "year",
        "display_name": "Year",
        "type": "integer"
      }
    ],
    "plot": {
      "x": "Study Leads",
      "y": "Count",
      "fill": "Assay",
      "facet": "Year"
    }
  },
  "data_focus": {
    "columns": [
      {
        "name": "studyName",
        "display_name": "Study Name",
        "type": "character"
      },
      {
        "name": "assay",
        "display_name": "Assays Used",
        "type": "character"
      },
      {
        "name": "resourceType",
        "display_name": "Resource Added",
        "type": "character"
      },
      {
        "name": "species",
        "display_name": "Species Used",
        "type": "character"
      },
      {
        "name": "tumorType",
        "display_name": "Tumor Types Investigated",
        "type": "character"
      }
    ],
    "plot": {
      "x": "Study Name",
      "fill": [
        "Assays Used",
        "Resource Added",
        "Species Used",
        "Tumor Types Investigated"
      ]
    },
    "tooltips": [
      "count",
      "fill"
    ],
    "empty_table_message": "The investigators have not uploaded data for this study yet. Please check back later."
  },
  "study_timeline": {
    "columns": [
      {
        "name": "studyName",
        "display_name": "Study Name",
        "type": "character"
      },
      {
        "name": "resourceType",
        "display_name": "Resource Type",
        "type": "character"
      },
      {
        "name": "year",
        "display_name": "Year",
        "type": "integer"
      },
      {
        "name": "month",
        "display_name": "Month",
        "type": "factor"
      }
    ],
    "plot": {
      "x": "Study Name",
      "fill": "Resource Type",
      "facet": [
        "Year",
        "Month"
      ]
    },
    "tooltips": [
      "count",
      "fill"
    ],
    "empty_table_message": "The investigator/investigators has/have not uploaded any files yet. Please check back later."
  },
  "publication_status": {
    "table": "publications",
    "filter_column": "studyName",
    "columns": [
      {
        "name": "year",
        "display_name": "Year",
        "type": "integer"
      },
      {
        "name": "studyName",
        "display_name": "Study Name",
        "type": "list:character"
      }
    ],
    "plot": {
      "x": "Year",
      "fill": "Study Name"
    },
    "empty_table_message": "This study has no associated publications yet.",
    "tooltips": [
      "count",
      "fill"
    ]
  },
  "study_details": {
    "columns": [
      {
        "name": "studyId",
        "type": "character",
        "display_name": "Unique Study ID"
      },
      {
        "name": "studyStatus",
        "type": "character",
        "display_name": "Study Status"
      },
      {
        "name": "dataStatus",
        "type": "character",
        "display_name": "Data Status"
      },
      {
        "name": "summary",
        "type": "character"
      },
      {
        "name": "diseaseFocus",
        "type": "list:character",
        "display_name": "Disease Focus"
      }
    ]
  }
}
